{
  "name": "js-slot-machine",
  "version": "0.1.0",
  "homepage": "https://marianapatcosta.github.io/js-slots-cra",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "craco": "^0.0.3",
    "gh-pages": "^6.0.0",
    "gsap": "^3.12.2",
    "i18next": "^23.5.1",
    "nanoid": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "typescript": "^5.2.2",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "lint": "eslint --max-warnings=0 src",
    "validate-ts": "tsc --project ./tsconfig.json",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:e2e": "cypress open",
    "gen-scss-types": "typed-scss-modules src --additionalData \"@import './assets/styles/_index.scss';\" --nameFormat none --exportType default",
    "deploy": "gh-pages -d build",
    "worker": "webpack --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/react-transition-group": "^4.4.6",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "eslint-config-react-app": "^7.0.1",
    "jest": "^29.7.0",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "ts-jest": "^29.1.1",
    "typed-scss-modules": "^7.1.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "cracoConfig": "craco.config.js",
  "jest": {
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/src/$1"
    }
  }
}
